[run]
source = src
omit = 
    */tests/*
    */venv/*
    */virtualenv/*
    */.venv/*
    */site-packages/*
    */migrations/*
    */settings/*
    */manage.py
    */wsgi.py
    */asgi.py
    */conftest.py
    */test_*.py
    */__pycache__/*
    */node_modules/*
    */static/*
    */media/*
    */locale/*
    */fixtures/*
    */scripts/*
    */docs/*
branch = True
parallel = True

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = FinBot ML Analytics Coverage Report

[xml]
output = coverage.xml