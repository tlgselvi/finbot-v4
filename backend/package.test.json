{
  "name": "finbot-analytics-backend",
  "version": "1.0.0",
  "description": "FinBot AI-Powered Financial Analytics Backend with Comprehensive Tests",
  "main": "app.js",
  "scripts": {
    "test": "node tests/runTests.js",
    "test:unit": "mocha 'tests/**/*.test.js' --timeout 10000 --recursive",
    "test:integration": "mocha 'tests/integration/**/*.test.js' --timeout 30000 --recursive",
    "test:ml": "mocha 'tests/ml/**/*.test.js' --timeout 20000 --recursive",
    "test:e2e": "mocha 'tests/e2e/**/*.test.js' --timeout 60000 --recursive",
    "test:watch": "mocha 'tests/**/*.test.js' --watch --recursive",
    "test:coverage": "nyc --reporter=html --reporter=text mocha 'tests/**/*.test.js' --recursive",
    "test:performance": "node tests/performance/performanceTests.js",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "test:quick": "mocha 'tests/**/*.test.js' --grep '@quick' --recursive",
    "test:slow": "mocha 'tests/**/*.test.js' --grep '@slow' --timeout 60000 --recursive",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "node app.js",
    "start:dev": "nodemon app.js",
    "start:test": "NODE_ENV=test node app.js",
    "db:seed": "node scripts/seedTestData.js",
    "db:clean": "node scripts/cleanTestData.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1",
    "axios": "^1.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "redis": "^4.6.7",
    "bull": "^4.11.3"
  },
  "devDependencies": {
    "mocha": "^10.2.0",
    "chai": "^4.3.8",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3",
    "nyc": "^15.1.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "nodemon": "^3.0.1",
    "chalk": "^4.1.2",
    "mongodb-memory-server": "^8.15.1",
    "faker": "^5.5.3",
    "artillery": "^2.0.0",
    "jest": "^29.6.4",
    "puppeteer": "^21.1.1"
  },
  "nyc": {
    "exclude": [
      "tests/**",
      "coverage/**",
      "node_modules/**",
      "*.config.js"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 75
  },
  "mocha": {
    "timeout": 10000,
    "recursive": true,
    "exit": true,
    "colors": true,
    "reporter": "spec"
  },
  "eslintConfig": {
    "extends": ["airbnb-base"],
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "rules": {
      "no-console": "off",
      "consistent-return": "off",
      "func-names": "off",
      "object-shorthand": "off",
      "no-process-exit": "off",
      "no-param-reassign": "off",
      "no-return-await": "off",
      "no-underscore-dangle": "off",
      "security/detect-object-injection": "off"
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "fintech",
    "analytics",
    "ai",
    "machine-learning",
    "financial-planning",
    "budget-optimization",
    "goal-tracking",
    "insights",
    "testing"
  ],
  "author": "FinBot Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/finbot/analytics-backend.git"
  },
  "bugs": {
    "url": "https://github.com/finbot/analytics-backend/issues"
  },
  "homepage": "https://github.com/finbot/analytics-backend#readme"
}