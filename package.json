{
  "name": "finbot-ai-analytics",
  "version": "1.0.0",
  "description": "AI-Powered Financial Analytics Platform",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "jest --coverage --watchAll=false && playwright test",
    "test:frontend": "jest --testPathPattern=tests/analytics",
    "test:mobile": "jest --testPathPattern=tests/mobile",
    "test:goal-tracking": "jest --testPathPattern=GoalTracking",
    "dev:backend": "cd backend && npm run dev",
    "dev:mobile": "cd src/mobile && expo start",
    "build:docker": "docker build -t finbot-ai-analytics .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "migrate": "cd backend && npm run migrate",
    "seed": "cd backend && npm run seed",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "playwright:install": "playwright install"
  },
  "keywords": [
    "fintech",
    "ai",
    "machine-learning",
    "financial-analytics",
    "react",
    "next.js",
    "react-native",
    "typescript"
  ],
  "author": "FinBot Team",
  "license": "MIT",
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "recharts": "^2.8.0",
    "lucide-react": "^0.294.0",
    "framer-motion": "^10.16.0",
    "react-hook-form": "^7.47.0",
    "zod": "^3.22.0",
    "@hookform/resolvers": "^3.3.0",
    "date-fns": "^2.30.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.0",
    "@playwright/test": "^1.40.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "prettier": "^3.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/finbot-ai-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/finbot-ai-analytics/issues"
  },
  "homepage": "https://github.com/yourusername/finbot-ai-analytics#readme"
}