{
  "ml_system_overview": {
    "dashboard": {
      "id": null,
      "title": "ML System Overview",
      "tags": ["ml", "finbot", "overview"],
      "timezone": "browser",
      "panels": [
        {
          "id": 1,
          "title": "System CPU Usage",
          "type": "stat",
          "targets": [
            {
              "expr": "system_cpu_usage_percent",
              "legendFormat": "CPU Usage %"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "color": {
                "mode": "thresholds"
              },
              "thresholds": {
                "steps": [
                  {"color": "green", "value": null},
                  {"color": "yellow", "value": 70},
                  {"color": "red", "value": 85}
                ]
              },
              "unit": "percent"
            }
          },
          "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
        },
        {
          "id": 2,
          "title": "System Memory Usage",
          "type": "stat",
          "targets": [
            {
              "expr": "system_memory_usage_percent",
              "legendFormat": "Memory Usage %"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "color": {
                "mode": "thresholds"
              },
              "thresholds": {
                "steps": [
                  {"color": "green", "value": null},
                  {"color": "yellow", "value": 75},
                  {"color": "red", "value": 90}
                ]
              },
              "unit": "percent"
            }
          },
          "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
        },
        {
          "id": 3,
          "title": "GPU Utilization",
          "type": "stat",
          "targets": [
            {
              "expr": "gpu_utilization_percent",
              "legendFormat": "GPU {{gpu_name}}"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "color": {
                "mode": "thresholds"
              },
              "thresholds": {
                "steps": [
                  {"color": "green", "value": null},
                  {"color": "yellow", "value": 80},
                  {"color": "red", "value": 95}
                ]
              },
              "unit": "percent"
            }
          },
          "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
        },
        {
          "id": 4,
          "title": "Active Alerts",
          "type": "stat",
          "targets": [
            {
              "expr": "count(ALERTS{alertstate=\"firing\"})",
              "legendFormat": "Active Alerts"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "color": {
                "mode": "thresholds"
              },
              "thresholds": {
                "steps": [
                  {"color": "green", "value": 0},
                  {"color": "yellow", "value": 1},
                  {"color": "red", "value": 5}
                ]
              }
            }
          },
          "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
        },
        {
          "id": 5,
          "title": "Model Inference Rate",
          "type": "graph",
          "targets": [
            {
              "expr": "rate(ml_model_inference_total[5m])",
              "legendFormat": "{{model_name}} - {{status}}"
            }
          ],
          "yAxes": [
            {
              "label": "Inferences/sec",
              "min": 0
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
        },
        {
          "id": 6,
          "title": "Model Inference Latency",
          "type": "graph",
          "targets": [
            {
              "expr": "histogram_quantile(0.95, rate(ml_model_inference_duration_seconds_bucket[5m]))",
              "legendFormat": "95th percentile"
            },
            {
              "expr": "histogram_quantile(0.50, rate(ml_model_inference_duration_seconds_bucket[5m]))",
              "legendFormat": "50th percentile"
            }
          ],
          "yAxes": [
            {
              "label": "Latency (seconds)",
              "min": 0
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
        },
        {
          "id": 7,
          "title": "Cache Hit Rate",
          "type": "graph",
          "targets": [
            {
              "expr": "rate(ml_cache_hits_total[5m]) / (rate(ml_cache_hits_total[5m]) + rate(ml_cache_misses_total[5m])) * 100",
              "legendFormat": "{{cache_type}} Hit Rate %"
            }
          ],
          "yAxes": [
            {
              "label": "Hit Rate %",
              "min": 0,
              "max": 100
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
        },
        {
          "id": 8,
          "title": "Model Accuracy",
          "type": "graph",
          "targets": [
            {
              "expr": "ml_model_accuracy",
              "legendFormat": "{{model_name}} v{{model_version}}"
            }
          ],
          "yAxes": [
            {
              "label": "Accuracy",
              "min": 0,
              "max": 1
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
        }
      ],
      "time": {
        "from": "now-1h",
        "to": "now"
      },
      "refresh": "30s"
    }
  },
  "business_metrics": {
    "dashboard": {
      "id": null,
      "title": "Business Metrics Dashboard",
      "tags": ["business", "finbot", "analytics"],
      "timezone": "browser",
      "panels": [
        {
          "id": 1,
          "title": "User Insights Generated",
          "type": "graph",
          "targets": [
            {
              "expr": "rate(business_user_insights_generated_total[5m])",
              "legendFormat": "{{insight_type}} - {{user_segment}}"
            }
          ],
          "yAxes": [
            {
              "label": "Insights/min",
              "min": 0
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
        },
        {
          "id": 2,
          "title": "Budget Optimizations",
          "type": "graph",
          "targets": [
            {
              "expr": "rate(business_budget_optimizations_total[5m])",
              "legendFormat": "{{optimization_type}}"
            }
          ],
          "yAxes": [
            {
              "label": "Optimizations/min",
              "min": 0
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
        },
        {
          "id": 3,
          "title": "Risk Assessments by Level",
          "type": "piechart",
          "targets": [
            {
              "expr": "sum by (risk_level) (business_risk_assessments_total)",
              "legendFormat": "{{risk_level}}"
            }
          ],
          "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8}
        },
        {
          "id": 4,
          "title": "Anomaly Detections",
          "type": "graph",
          "targets": [
            {
              "expr": "rate(business_anomaly_detections_total[5m])",
              "legendFormat": "{{anomaly_type}} - {{severity}}"
            }
          ],
          "yAxes": [
            {
              "label": "Anomalies/min",
              "min": 0
            }
          ],
          "gridPos": {"h": 8, "w": 16, "x": 8, "y": 8}
        },
        {
          "id": 5,
          "title": "Model Performance Trends",
          "type": "graph",
          "targets": [
            {
              "expr": "ml_model_accuracy",
              "legendFormat": "{{model_name}} Accuracy"
            },
            {
              "expr": "ml_model_drift_score",
              "legendFormat": "{{model_name}} Drift Score"
            }
          ],
          "yAxes": [
            {
              "label": "Score",
              "min": 0,
              "max": 1
            }
          ],
          "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
        }
      ],
      "time": {
        "from": "now-6h",
        "to": "now"
      },
      "refresh": "1m"
    }
  },
  "alerts_dashboard": {
    "dashboard": {
      "id": null,
      "title": "Alerts & Incidents",
      "tags": ["alerts", "incidents", "monitoring"],
      "timezone": "browser",
      "panels": [
        {
          "id": 1,
          "title": "Alert Status",
          "type": "alertlist",
          "targets": [],
          "options": {
            "showOptions": "current",
            "maxItems": 20,
            "sortOrder": 1
          },
          "gridPos": {"h": 12, "w": 12, "x": 0, "y": 0}
        },
        {
          "id": 2,
          "title": "Alert Frequency",
          "type": "graph",
          "targets": [
            {
              "expr": "increase(prometheus_notifications_total[1h])",
              "legendFormat": "Alerts per hour"
            }
          ],
          "yAxes": [
            {
              "label": "Count",
              "min": 0
            }
          ],
          "gridPos": {"h": 12, "w": 12, "x": 12, "y": 0}
        },
        {
          "id": 3,
          "title": "System Health Score",
          "type": "stat",
          "targets": [
            {
              "expr": "(1 - (count(ALERTS{alertstate=\"firing\"}) / 10)) * 100",
              "legendFormat": "Health Score %"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "color": {
                "mode": "thresholds"
              },
              "thresholds": {
                "steps": [
                  {"color": "red", "value": null},
                  {"color": "yellow", "value": 70},
                  {"color": "green", "value": 90}
                ]
              },
              "unit": "percent"
            }
          },
          "gridPos": {"h": 6, "w": 8, "x": 0, "y": 12}
        },
        {
          "id": 4,
          "title": "Mean Time to Resolution",
          "type": "stat",
          "targets": [
            {
              "expr": "avg(alert_resolution_time_seconds)",
              "legendFormat": "MTTR"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "unit": "s"
            }
          },
          "gridPos": {"h": 6, "w": 8, "x": 8, "y": 12}
        },
        {
          "id": 5,
          "title": "Alert Severity Distribution",
          "type": "piechart",
          "targets": [
            {
              "expr": "sum by (severity) (ALERTS{alertstate=\"firing\"})",
              "legendFormat": "{{severity}}"
            }
          ],
          "gridPos": {"h": 6, "w": 8, "x": 16, "y": 12}
        }
      ],
      "time": {
        "from": "now-24h",
        "to": "now"
      },
      "refresh": "30s"
    }
  }
}